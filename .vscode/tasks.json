{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Clean",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"command": "make clean"
		},
		{
			"label": "Run",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"command": "make run",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}