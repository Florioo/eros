{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Cmake",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"command": "cmake -B build -S .",
			"problemMatcher": []
		},
		{
			"label": "Run",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"command": "cmake --build build --target run",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [],
			"dependsOn": "Cmake"
		},
		{
			"label": "Run2",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"command": "cmake --build build --target run2",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [],
			"dependsOn": "Cmake"
		}
	]
}